generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Restaurant {
  id           String  @id @default(uuid())
  name         String
  type         String
  capacityBase Int
  cash         Decimal @default(0)
}

model Scenario {
  id          String @id
  name        String
  description String?
}
